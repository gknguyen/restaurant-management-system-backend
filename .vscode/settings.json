{
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  },
  "files.associations": {
    "tslint.json": "jsonc"
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "cSpell.ignorePaths": [
    "**/package-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    "**/.git/objects/**",
    ".vscode",
    "**.json"
  ],
  "files.autoSave": "onFocusChange",
  "cSpell.words": [
    "oracledb"
  ]
}