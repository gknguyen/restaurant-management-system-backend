{
  "name": "restaurant_management_system",
  "version": "1.0.0",
  "private": true,
  "description": "Restaurant management app backend",
  "author": "Giang Nguyen",
  "license": "ISC",
  "scripts": {
    "start:dev": "cross-env DEBUG=restaurant_management_system:server NODE_ENV=development ts-node-dev --respawn --transpileOnly ./src/server --watch ./src",
    "start:prod": "cross-env NODE_ENV=production tsc && node ./lib/server.js",
    "build": "cross-env NODE_ENV=production tsc",
    "ci": "yarn install --frozen-lockfile --ignore-scripts",
    "ci:prod": "yarn install --frozen-lockfile --ignore-scripts --prod",
    "fix:port": "killall -s KILL node",
    "prettier": "prettier --write ./src/**/*.ts",
    "complex": "ts-plato --recurse --quiet --eslint ./.eslintrc --noempty --dir ./.reports/complex ./src",
    "eslint": "eslint --quiet ./src --ext .js,.ts",
    "review": "concurrently \"npm run prettier\" \"npm run eslint:BE\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run review"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.653.0",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "concurrently": "^6.2.0",
    "config": "^3.3.0",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "crypto-js": "^4.0.0",
    "cryptojs": "^2.5.3",
    "csurf": "^1.11.0",
    "debug": "~4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^5.3.0",
    "express-session": "^1.17.0",
    "google-auth-library": "^7.3.0",
    "googleapis": "^81.0.0",
    "helmet": "^3.22.0",
    "http": "0.0.0",
    "http-status": "^1.4.2",
    "ldapts": "^2.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.28",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.1.0",
    "node-logger": "0.0.1",
    "nodemon": "^2.0.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "redis-clustr": "^1.4.3",
    "sequelize": "^6.6.5",
    "supports-color": "^7.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/bluebird": "^3.5.32",
    "@types/compression": "^1.7.0",
    "@types/config": "0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.0.2",
    "@types/csurf": "^1.11.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.3",
    "@types/express-jwt": "0.0.42",
    "@types/express-rate-limit": "^5.1.3",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "^0.0.45",
    "@types/moment-timezone": "^0.5.13",
    "@types/morgan": "^1.9.0",
    "@types/multer": "^1.4.2",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.3",
    "@types/passport-google-oauth20": "^2.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/supports-color": "^5.3.0",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "chai": "^4.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.8.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-unicorn": "^34.0.1",
    "husky": "^7.0.1",
    "mocha": "^7.1.1",
    "prettier": "^2.0.5",
    "prettierrc": "^0.0.0-5",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "ts-plato": "^0.0.6",
    "typescript": "^3.8.3"
  }
}
